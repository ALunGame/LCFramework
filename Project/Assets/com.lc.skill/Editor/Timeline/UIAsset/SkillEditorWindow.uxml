            </ui:VisualElement>
            <ui:VisualElement name="RightScrollBox" style="height: auto; width: 13px; border-left-color: rgb(0, 0, 0); border-right-color: rgb(0, 0, 0); border-top-color: rgb(0, 0, 0); border-bottom-color: rgb(0, 0, 0); border-right-width: 0; flex-direction: row; border-left-width: 0; border-top-width: 0; border-bottom-width: 0; max-height: none; min-height: auto; flex-grow: 1; position: relative; right: auto; max-width: 13px; min-width: 13px; display: flex;">
                <ui:ScrollView name="RightScroll" mode="Vertical" horizontal-scroller-visibility="Hidden" vertical-scroller-visibility="AlwaysVisible" touch-scroll-type="Unrestricted" elasticity="0.1" scroll-deceleration-rate="0.135" style="height: auto; width: auto; max-width: none; min-width: auto; flex-grow: 1;" />
            </ui:VisualElement>
        </ui:VisualElement>
        <ui:VisualElement name="BottomScroll" style="flex-direction: row; min-width: auto; width: auto; height: 13px; border-left-color: rgb(0, 0, 0); border-right-color: rgb(0, 0, 0); border-top-color: rgb(0, 0, 0); border-bottom-color: rgb(0, 0, 0); border-left-width: 1px; border-right-width: 1px; align-items: stretch; max-height: 13px; min-height: 13px; border-bottom-width: 1px; flex-grow: 1; bottom: auto; position: relative;">
            <ui:VisualElement name="Empty" style="height: 13px; width: 200px; border-left-color: rgb(0, 0, 0); border-right-color: rgb(0, 0, 0); border-top-color: rgb(0, 0, 0); border-bottom-color: rgb(0, 0, 0); border-right-width: 1px; border-left-width: 0; border-top-width: 0; border-bottom-width: 0; max-width: 200px; min-width: 200px; min-height: 13px; max-height: 13px;" />
            <ui:VisualElement name="ScrollBox" style="height: 13px; width: auto; border-left-color: rgb(0, 0, 0); border-right-color: rgb(0, 0, 0); border-top-color: rgb(0, 0, 0); border-bottom-color: rgb(0, 0, 0); border-right-width: 0; flex-direction: row; border-left-width: 0; border-top-width: 0; border-bottom-width: 0; max-height: 13px; min-height: 13px; flex-grow: 1;">
                <ui:ScrollView name="Scroll" mode="Horizontal" horizontal-scroller-visibility="Auto" vertical-scroller-visibility="Hidden" touch-scroll-type="Unrestricted" elasticity="0.1" scroll-deceleration-rate="0.135" style="height: auto;" />
            </ui:VisualElement>
            <ui:VisualElement name="Empty" style="height: 13px; width: 13px; border-left-color: rgb(0, 0, 0); border-right-color: rgb(0, 0, 0); border-top-color: rgb(0, 0, 0); border-bottom-color: rgb(0, 0, 0); border-right-width: 0; border-left-width: 1px; border-top-width: 0; border-bottom-width: 0; max-width: 13px; min-width: 13px; min-height: 13px; max-height: 13px; position: absolute; right: 0;" />
        </ui:VisualElement>
    </ui:VisualElement>
