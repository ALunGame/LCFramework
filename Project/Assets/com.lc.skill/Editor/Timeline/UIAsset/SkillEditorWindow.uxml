            </ui:VisualElement>
        </ui:VisualElement>
        <ui:VisualElement name="Bottom" style="width: auto; flex-direction: row; flex-grow: 1; flex-shrink: 1; border-left-color: rgb(0, 0, 0); border-right-color: rgb(0, 0, 0); border-top-color: rgb(0, 0, 0); border-bottom-color: rgb(0, 0, 0); border-left-width: 1px; border-right-width: 1px; border-top-width: 2px; border-bottom-width: 1px;">
