<ui:UXML xmlns:ui="UnityEngine.UIElements" xmlns:uie="UnityEditor.UIElements" xsi="http://www.w3.org/2001/XMLSchema-instance" engine="UnityEngine.UIElements" editor="UnityEditor.UIElements" noNamespaceSchemaLocation="../../../../../../UIElementsSchema/UIElements.xsd" editor-extension-mode="True">
    <ui:VisualElement name="Panel" style="height: auto; width: auto; flex-grow: 1;">
        <ui:VisualElement name="Top" style="border-left-width: 1px; border-right-width: 1px; border-top-width: 1px; border-bottom-width: 1px; height: 30px; border-left-color: rgb(0, 0, 0); border-right-color: rgb(0, 0, 0); border-top-color: rgb(0, 0, 0); border-bottom-color: rgb(0, 0, 0); flex-direction: row; position: relative; top: auto; flex-grow: 0; flex-shrink: 0; max-height: 30px; min-height: 30px;">
            <ui:VisualElement name="Left" style="height: 30px; max-height: 30px; min-height: 30px; width: 200px; max-width: 200px; min-width: 200px; border-left-color: rgb(0, 0, 0); border-right-color: rgb(0, 0, 0); border-top-color: rgb(0, 0, 0); border-bottom-color: rgb(0, 0, 0); border-right-width: 1px; flex-direction: row;">
                <ui:Label text="演示节点：" display-tooltip-when-elided="true" style="justify-content: center; -unity-text-align: middle-left; -unity-font-style: bold; color: rgb(255, 255, 255);" />
                <uie:ObjectField type="UnityEngine.GameObject, UnityEngine.CoreModule" allow-scene-objects="false" name="PreviewGo" style="width: 127px; flex-grow: 1; -unity-text-align: middle-left; margin-top: 3px; margin-bottom: 5px;" />
            </ui:VisualElement>
            <ui:VisualElement name="Right" style="height: 30px; flex-grow: 1; flex-direction: row; justify-content: center;">
                <ui:Label text="资源:" display-tooltip-when-elided="true" style="-unity-text-align: middle-center; -unity-font-style: bold; margin-bottom: 5px;" />
                <uie:ObjectField name="AssetObj" style="margin-top: 3px; margin-bottom: 5px;" />
                <ui:Button text="保存" display-tooltip-when-elided="true" name="SaveBtn" style="height: 25px; width: 50px; align-items: stretch; left: auto; position: relative; right: auto;" />
            </ui:VisualElement>
        </ui:VisualElement>
        <ui:VisualElement name="Middle" style="flex-direction: row; min-width: auto; width: auto; height: 50px; border-left-color: rgb(0, 0, 0); border-right-color: rgb(0, 0, 0); border-top-color: rgb(0, 0, 0); border-bottom-color: rgb(0, 0, 0); border-left-width: 1px; border-right-width: 1px; align-items: stretch; max-height: 50px; min-height: 50px; flex-shrink: 1; flex-grow: 1;">
            <ui:VisualElement name="PlayArea" style="height: 50px; width: 200px; border-left-color: rgb(0, 0, 0); border-right-color: rgb(0, 0, 0); border-top-color: rgb(0, 0, 0); border-bottom-color: rgb(0, 0, 0); border-right-width: 1px; border-left-width: 0; border-top-width: 0; border-bottom-width: 0; max-width: 200px; min-width: 200px; min-height: 50px; max-height: 50px; flex-shrink: 0; flex-direction: column;">
                <ui:VisualElement name="PlayAreaTop" style="height: 25px; max-height: 25px; min-height: 25px; flex-direction: row; justify-content: center; border-left-color: rgb(0, 0, 0); border-right-color: rgb(0, 0, 0); border-top-color: rgb(0, 0, 0); border-bottom-color: rgb(0, 0, 0); border-bottom-width: 1px;">
                    <ui:IMGUIContainer name="StartFrameBtn" style="width: 35px;" />
                    <ui:IMGUIContainer name="PreFrameBtn" style="width: 35px;" />
                    <ui:IMGUIContainer name="PlayBtn" style="width: 35px;" />
                    <ui:IMGUIContainer name="LaterFrameBtn" style="width: 35px;" />
                    <ui:IMGUIContainer name="EndFrameBtn" style="width: 35px;" />
                </ui:VisualElement>
                <ui:VisualElement name="PlayAreaBottom" style="height: 25px; max-height: 25px; min-height: 25px; flex-direction: row; justify-content: center;">
                    <ui:IMGUIContainer name="AddGroupBtn" style="position: absolute; left: 0; top: 0; bottom: 0; height: 25px; max-height: 25px; min-height: 25px; width: 100px; max-width: 100px; min-width: 100px;" />
                    <ui:VisualElement name="Frame" style="height: 25px; max-height: 25px; min-height: 25px; flex-direction: row; width: 100px; max-width: 100px; min-width: 100px; right: 0; position: absolute; justify-content: flex-end; border-left-color: rgb(0, 0, 0); border-right-color: rgb(0, 0, 0); border-top-color: rgb(0, 0, 0); border-bottom-color: rgb(0, 0, 0); border-left-width: 0;">
                        <uie:IntegerField value="42" name="CurrFrame" style="flex-grow: 1; -unity-text-align: middle-right;" />
                        <ui:Label text="/" display-tooltip-when-elided="true" style="-unity-text-align: middle-center; -unity-font-style: bold;" />
                        <ui:Label text="20000" display-tooltip-when-elided="true" name="TotalFrame" style="-unity-text-align: middle-center; -unity-font-style: bold;" />
                    </ui:VisualElement>
                </ui:VisualElement>
            </ui:VisualElement>
            <ui:VisualElement name="FrameRuler" style="height: 50px; width: auto; border-left-color: rgb(0, 0, 0); border-right-color: rgb(0, 0, 0); border-top-color: rgb(0, 0, 0); border-bottom-color: rgb(0, 0, 0); border-right-width: 0; flex-direction: row; border-left-width: 0; border-top-width: 0; border-bottom-width: 0; max-height: 50px; min-height: 50px; flex-grow: 1; flex-shrink: 1;">
